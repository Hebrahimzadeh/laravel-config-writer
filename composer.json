{
    "name": "hebrahimzadeh/laravel-config-writer",
    "description": "Laravel provider to be able to rewrite configuration",
    "keywords": ["october cms", "october", "laravel", "config", "write", "provider"],
    "license": "MIT",
    "authors": [
        {
            "name": "Hossein Ebrahimzdeh",
            "email": "abi.hossein@gmeil.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.19"
    },
    "autoload": {
        "psr-4": {
            "October\\Rain\\Config\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "October\\Rain\\Config\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "October\\Rain\\Config\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
